{
	"linters": {
		"disable-all": true,
		"enable": [
			"govet",
			"revive",
			"goimports",
			"gofmt"
		]
	},
	"run": {
		"deadline": "5m"
	},
	"issues": {
		"exclude-rules": [
			{
				"linters": "typecheck",
				"text": "imported but not used"
			},
			{
				"linters": "revive",
				"text": "should have comment"
			},
			{
				"linters": "revive",
				"text": "unused-parameter"
			}
		]
	}
}
